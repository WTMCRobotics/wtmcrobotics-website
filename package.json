{
  "name": "wtmcrobotics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:emulator": "vue-cli-service serve --port 5000 --mode emulator",
    "build": "vue-cli-service build",
    "build:emulator": "vue-cli-service build --mode emulator",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && firebase deploy --except functions",
    "build:both": "npm run build && npm run build --prefix ./functions",
    "cypress:open": "cypress open",
    "emulator:start": "npm run build --prefix ./functions && firebase emulators:start --only \"functions,firestore\""
  },
  "dependencies": {
    "@sophosoft/vue-meta-decorator": "^0.1.1",
    "core-js": "^3.16.4",
    "firebase": "^7.24.0",
    "firebaseui": "^4.8.1",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-meta": "^2.4.0",
    "vue-property-decorator": "^8.4.2",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.5.2",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.5.8",
    "vuex": "^3.6.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@mdi/js": "^5.9.55",
    "@types/resize-observer-browser": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-pwa": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "cypress": "^6.9.1",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "sass": "^1.38.2",
    "sass-loader": "^8.0.2",
    "typescript": "^4.1.6",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  }
}
